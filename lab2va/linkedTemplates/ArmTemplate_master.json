{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "lab2va"
		},
		"ADLS_cathrinewadlsva_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_cathrinewadlsva'"
		},
		"AzureSqlDatabaseLinkedList_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseLinkedList'"
		},
		"ADLS_cathrinewadlsva_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cathrinewadlsva.dfs.core.windows.net"
		},
		"Http_Lego_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdn.rebrickable.com/media/downloads/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "lab2va_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_cathrinewadlsva_accountKey": {
						"value": "[parameters('ADLS_cathrinewadlsva_accountKey')]"
					},
					"AzureSqlDatabaseLinkedList_connectionString": {
						"value": "[parameters('AzureSqlDatabaseLinkedList_connectionString')]"
					},
					"ADLS_cathrinewadlsva_properties_typeProperties_url": {
						"value": "[parameters('ADLS_cathrinewadlsva_properties_typeProperties_url')]"
					},
					"Http_Lego_properties_typeProperties_url": {
						"value": "[parameters('Http_Lego_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}